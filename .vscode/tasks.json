{
    "version": "2.0.0",
    "tasks": [
        {
            "label": "generate",
            "type": "shell",
            "command": "go generate",
            "presentation": {
                "echo": false,
                "reveal": "silent",
                "clear": true
            },
            "options": {
                "env": {
                    "GOPATH": "${workspaceFolder}"
                }
            }
        },
        {
            "label": "postdebugKill",
            "type": "process",
            "command": [
                "${command:workbench.action.tasks.terminate}",
                "${command:workbench.action.acceptSelectedQuickOpenItem}",
            ],
        },
    ]
}